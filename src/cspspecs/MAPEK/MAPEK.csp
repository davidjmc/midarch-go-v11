P1 = (PLANNER|||EXECUTOR|||CORRECTIVEMONITOR|||EVOLUTIVEMONITOR|||PROACTIVEMONITOR|||MONITOR|||ANALYSER)
[|{|InvP,InvR|}|]
(T4[[InvP.planner <- InvR.t4,InvR.executor <- InvP.t4]]|||T1[[InvP.correctiveMonitor <- InvR.t1,InvR.monitor <- InvP.t1,InvP.evolutiveMonitor <- InvR.t1,InvR.monitor <- InvP.t1,InvP.proactiveMonitor <- InvR.t1,InvR.monitor <- InvP.t1]]|||T2[[InvP.monitor <- InvR.t2,InvR.analyser <- InvP.t2]]|||T3[[InvP.analyser <- InvR.t3,InvR.planner <- InvP.t3]])
