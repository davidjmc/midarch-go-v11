datatype PROCNAMES = analyser | correctiveMonitor | evolutiveMonitor | planner | executor | proactiveMonitor | monitor | t1 | t2 | t3 | t4 
channel I_PosInvP,I_GenerateEvent
channel InvR,InvP : PROCNAMES
ANALYSER = InvP.t2 -> I_PosInvP -> InvR.t3 -> ANALYSER
